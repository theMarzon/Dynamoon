{
  "name": "dynamoon",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "yarn run build && yarn run start",
    "start": "node ./build/index.js",
    "build": "swc --config-file ./.swcrc.json ./source/ -d ./build/",
    "check": "eslint ./ --ext .js,.ts",
    "fix": "eslint ./ --fix --ext .js,.ts"
  },
  "dependencies": {
    "discord.js": "^14.0.1",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.215",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "typescript": "^4.7.4"
  }
}
